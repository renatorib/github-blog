{
  "name": "@rena.to/github-blog",
  "version": "0.4.1",
  "description": "Turn your github issues in CMS for your blog",
  "main": "dist/index.js",
  "repository": "https://github.com/renatorib/github-blog",
  "author": "Renato Ribeiro <hi@rena.to>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf ./dist",
    "codegen": "graphql-codegen -r dotenv-flow/config",
    "type-check": "tsc --noEmit",
    "dev": "yarn codegen --watch",
    "build": "yarn codegen && yarn clean && tsc",
    "e2e": "yarn build && yarn jest --testPathPattern=e2e",
    "prepublishOnly": "yarn e2e",
    "debug": "node -r dotenv-flow/config src/tmp/debug.js",
    "docs": "typedoc src/public-types.ts --readme none --githubPages false"
  },
  "lint-staged": {
    "*.{ts,css,md}": "prettier --write"
  },
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "gray-matter": "^4.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-graphql-request": "^3.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.15",
    "@types/jest": "^26.0.23",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.23.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.22.4",
    "typedoc-plugin-markdown": "^3.11.2",
    "typescript": "^4.2.3"
  }
}
